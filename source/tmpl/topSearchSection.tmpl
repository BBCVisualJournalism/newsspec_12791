<div class="ns12791_topSearchHolder">
	<div class="ns12791_searchContentsHolder">
		<h1>{{= musicalTwinTowns }}</h1>
		<h4>{{= theSongsCitiesAreListeningTo }}</h4>
		
		<div class="ns12791_searchBoxHolder">
			
			<div class="ns12791_cityFreeTextSearchHolder" id="ns12791_cityFreeTextSearchHolder">
				<h4>{{= enterYourNearestCity }}</h4>
				
				<div class="city-search--inputs">
	                <div class="city-search--autocomplete">
	                    <input type="text" placeholder="Your city" id="city-search--text-input" class="city-search--text-input" />
	                </div>
	                <button class="city-search--submit disabled">{{= search }}</button>
	                <span class="ns12791_alternateSearchSpan" id="ns12791_cantFindCity">{{= cantFindYourCity }}</span>
	            </div>
			</div>
			
			<div class="ns12791_countryFreeTextSearchHolder" id="ns12791_countryFreeTextSearchHolder" style="display:none;">
				<h4>{{= enterYourCountry }}</h4>
				
				<div class="country-search--inputs">
	                <div class="country-search--autocomplete">
	                    <input type="text" placeholder="Your country" id="country-search--text-input" class="country-search--text-input" />
	                </div>
	                <select class="city-search--dropdown-input disabled" id="city-search--dropdown-input"></select>
	                <button class="country-search--submit disabled">{{= search }}</button>
	                <span class="ns12791_alternateSearchSpan" id="ns12791_backToOriginalSearch">{{= backToOriginalSearch }}</span>
	            </div>
			</div>
		</div>

	</div>
</div>